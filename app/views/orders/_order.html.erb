<div id="<%= dom_id order %>">
  <p>
    <u>Order:</u>
  </p>

  <p>
    <strong>Customer:</strong>
    <%= order.customer_id %>
  </p>

  <p>
    <strong>Total:</strong>
    <%= order.total %>
  </p>

  <p>
    <strong>Order date:</strong>
    <%= order.order_date %>
